{
  "manifest_version": 2,
  "name": "Krypto",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js", "jquery.min.js", "bootstrap.min.js", "index.js"]
    }
  ],
  "browser_action": {
  "default_icon": "bitcoin.png",
  "default_popup": "index.html"
}
}
